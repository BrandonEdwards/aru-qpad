@article{palacio_costs_2020,
	title = {The costs of ignoring species detectability on functional diversity estimation},
	volume = {137},
	issn = {0004-8038, 1938-4254},
	url = {https://academic.oup.com/auk/article/doi/10.1093/auk/ukaa057/5898991},
	doi = {10.1093/auk/ukaa057},
	abstract = {Functional diversity (FD) approaches have been increasingly used to understand ecosystem functioning in bird communities. These approaches typically rely on the assumption that species are perfectly detected in the field, despite the fact that imperfect detection represents a ubiquitous source of bias in biodiversity studies. This may be notably important in FD studies, because detection may depend on the functional traits used to compute FD metrics. However, little effort has been devoted to account for imperfect detection in FD studies, and therefore the degree to which species traits and detectability affects FD remains poorly understood. We predict that observed FD metrics may either underestimate or overestimate detection-corrected FD, because FD has multiple independent dimensions with different data properties. We assessed whether detection was related to bird traits (body mass, diet, and foraging stratum), accounting for habitat type, season, and phylogeny. We then used a multi-species occupancy model to obtain detection-corrected FD metrics (functional richness [FRic], functional evenness [FEve], and functional divergence [FDiv]), and compared observed and detection-corrected FD estimates in bird communities from east-central Argentina. Some functional types of birds (raptors and insectivores) were more easily overlooked, whereas others (seed and leaf eaters) were more easily detected. Some observed FD metrics underestimated detection-corrected FD (FRic and FDiv), whereas some others (FEve) overestimated detection-corrected FD. Both observed and detection-corrected FRic revealed differences between seasons, but not between habitat types. However, detection-corrected FEve and FDiv showed differences between seasons, contrary to observed estimates. Our results indicate that failure to account for unequal ease of detecting species can lead to erroneous estimates of FD because some functional types of birds are more easily overlooked. We outline some guidelines to help ornithologists identifying under which circumstances detection may be a concern and warn against the indiscriminate use of FD metrics without accounting for species detection.},
	language = {en},
	number = {4},
	urldate = {2022-01-05},
	journal = {The Auk},
	author = {Palacio, Facundo Xavier and Maragliano, René E and Montalti, Diego},
	month = dec,
	year = {2020},
	pages = {ukaa057},
	file = {Palacio et al. - 2020 - The costs of ignoring species detectability on fun.pdf:/home/brandon/Zotero/storage/2NZSB7FT/Palacio et al. - 2020 - The costs of ignoring species detectability on fun.pdf:application/pdf},
}

@article{solymos_phylogeny_2018,
	title = {Phylogeny and species traits predict bird detectability},
	volume = {41},
	issn = {09067590},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/ecog.03415},
	doi = {10.1111/ecog.03415},
	language = {en},
	number = {10},
	urldate = {2021-07-15},
	journal = {Ecography},
	author = {Sólymos, Péter and Matsuoka, Steven M. and Stralberg, Diana and Barker, Nicole K. S. and Bayne, Erin M.},
	month = oct,
	year = {2018},
	keywords = {traits},
	pages = {1595--1603},
	annote = {This paper explores the use of phylogeny and species traits in predicting singing rate and detection distance, both as previously calculated from Solymos et al. (2013). That is, can be use information about species traits and phylogeny to gain better estimates of singing rate and/or detection distance? The authors used 5 species traits here: migration strategy, body mass, habitat associations, minimum sound frequency, and minimum nest height. The authors regressed log(singing rate) and log(detection distance) against these. The authors also used phylogenetic mixed models to estimate \${\textbackslash}lambda\$, the strength of phylogeneitic effects. The authors found that both singing rate and detection distance were in fact correlated to traits and phylogeny, but that strength differed for each one. The combination of phylogeny and species traits best explained singing rate, with phylogeny having the stronger predictive power. They found that long-distant migrants sang at a higher rate than residents. The detection distance was best explained by the traits only model, with large detection distances characterized by larger body mass, migratory species, and species associated with open habitats. The authors conclude that considering traits as somethgn that influences detection distance and singing rate would be a useful tool to be able to group birds to run as a multi-species model, alleviating small sample sizes in some species. That is, in my NA-POPS project, I could group sets of species taht share similar traits, and run that as a multi-species model to share information across those species to allow for data-poor species to shrink toward a mean caused by data-rich species.},
	annote = {This study investigated the effects of bird traits and bird phylogeny on detectability. Most important results were that the traits of song pitch and migratory status appeared to have the greatest effect on a bird's availability (propensity to give a cue), and the traits of song pitch, body size, migratory status, and habitat affinity appeared to have the greatest effect on an observer's ability to perceive the bird. This grouping of detectability based on traits could be a useful way to model detectability of several brids such as in NA-POPS.},
	file = {Sólymos et al. - 2018 - Phylogeny and species traits predict bird detectab.pdf:/home/brandon/Zotero/storage/9RFRUHCU/Sólymos et al. - 2018 - Phylogeny and species traits predict bird detectab.pdf:application/pdf},
}

@article{johnston_species_2014,
	title = {Species traits explain variation in detectability of {UK} birds},
	volume = {61},
	issn = {0006-3657, 1944-6705},
	url = {http://www.tandfonline.com/doi/abs/10.1080/00063657.2014.941787},
	doi = {10.1080/00063657.2014.941787},
	abstract = {Methods We estimated the detectability of 195 species of birds in the UK using distance sampling methods and examined the average detectability of genetically related groups. We tested the significance of species traits in describing variation in detectability, whilst controlling for phylogenetic relationships.
Results Passeriformes had the lowest median detectability of 0.37 and Charadriiformes the highest median detectability of 0.65, of the seven largest orders considered. Species most associated with closed habitats such as woodland and urban areas had the lowest detectability. Smaller species had lower detectability than larger species.
Conclusion Heterogeneity in species detectability could lead to biased conclusions, particularly when calculating multi-species indices such as species richness or diversity. Accounting for detectability will be most important in studies that cover a wide range of habitat types or a diverse spread of taxa.},
