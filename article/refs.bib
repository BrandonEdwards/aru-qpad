	pages = {268--282},
	annote = {Need to re-read this one, but is pointed to many times by PIF as the initial "EDR estimation" paper},
	file = {Matsuoka et al. - 2012 - Using binomial distance-sampling models to estimat.pdf:/home/brandon/Zotero/storage/529CEK4X/Matsuoka et al. - 2012 - Using binomial distance-sampling models to estimat.pdf:application/pdf},
